            {% extends "OCPAppliBundle::layout.html.twig"%}
            {% block title %}{{ parent() }}{% endblock %}
            {% block body %}
                <br><br><br> 
                <div align="center"> <h2>liste des destockage</h2></div>
                     
                    <div class="well well-small " align="center">
    <table border="1" width="50%"  class="table table-bordered">
                
                    <tr>
                        <th>id</th>
                        <th>date</th>
                        <th>modifier</th>
                        <th>supprimer</th>
                    </tr>
                    
                   {%  for p in  destockage %}
                       
                       <tr class="success">
                           <td>{{p.id}}</td>
                           <td>{{p.date|date('d/m/Y')}}</td>
                           <td><a class="btn btn-info" href="{{ path('ocp_appli_editdestockage',{'id':p.id})}}">modifier<a></td>
                                       <td><a class="btn btn-danger" href="{{ path('ocp_appli_deletedestockage',{'id':p.id})}}">supprimer</td>
                       </tr>
                       
                       {%endfor%}
                    
                        
                    
                </table>
                    </div>
             
            {% endblock %}