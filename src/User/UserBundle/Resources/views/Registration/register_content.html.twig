{% trans_default_domain 'FOSUserBundle' %}

<div class="container  col-lg-offset-3 col-md-5">
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} class="well" method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary btn-block" />
    </div>
</form>
</div>
